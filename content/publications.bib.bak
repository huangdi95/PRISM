@inproceedings{smith2024constructive,
  title = {A Constructive Proof of the Generalized Riemann Hypothesis for Function Fields over Finite Constants},
  booktitle = {Proceedings of the 2024 International Congress of Mathematicians (ICM)},
  author = {John Smith# and Jiale Liu#* and Alan Turing},
  year = {2024},
  month = jun,
  pages = {205--218},
  publisher = {EMS Press},
  address = {Paris, France},
  paperlink = {10.4171/ICM2024/102},
  urldate = {2024-12-02},
  isbn = {978-3-03719-202-8},
  abstract = {We present a novel constructive proof for the Generalized Riemann Hypothesis (GRH) specifically within the context of function fields over finite constants. Unlike previous existential approaches, our method utilizes the spectral interpretation of the Frobenius automorphism acting on Ã©tale cohomology groups. We explicitly construct the L-functions associated with algebraic curves and demonstrate that their zeros lie strictly on the critical line Re(s) = 1/2. This work bridges the gap between classical analytic number theory and geometric invariant theory, offering new insights into the distribution of prime polynomials. The constructive nature of our proof allows for the algorithmic verification of L-function properties for curves of genus g > 100, which was previously computationally intractable.},
  preview={example.png},
  keywords={Number Theory, Riemann Hypothesis, Function Fields, Algebraic Geometry}
}


